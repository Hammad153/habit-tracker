#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ahmad/Desktop/calmnest/node_modules/.pnpm/jest-expo@53.0.13_@babel+core@7.28.5_expo@54.0.29_jest@29.7.0_@types+node@25.0.1__react_98642b21943c34ce6f99732034af3868/node_modules/jest-expo/bin/node_modules:/home/ahmad/Desktop/calmnest/node_modules/.pnpm/jest-expo@53.0.13_@babel+core@7.28.5_expo@54.0.29_jest@29.7.0_@types+node@25.0.1__react_98642b21943c34ce6f99732034af3868/node_modules/jest-expo/node_modules:/home/ahmad/Desktop/calmnest/node_modules/.pnpm/jest-expo@53.0.13_@babel+core@7.28.5_expo@54.0.29_jest@29.7.0_@types+node@25.0.1__react_98642b21943c34ce6f99732034af3868/node_modules:/home/ahmad/Desktop/calmnest/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ahmad/Desktop/calmnest/node_modules/.pnpm/jest-expo@53.0.13_@babel+core@7.28.5_expo@54.0.29_jest@29.7.0_@types+node@25.0.1__react_98642b21943c34ce6f99732034af3868/node_modules/jest-expo/bin/node_modules:/home/ahmad/Desktop/calmnest/node_modules/.pnpm/jest-expo@53.0.13_@babel+core@7.28.5_expo@54.0.29_jest@29.7.0_@types+node@25.0.1__react_98642b21943c34ce6f99732034af3868/node_modules/jest-expo/node_modules:/home/ahmad/Desktop/calmnest/node_modules/.pnpm/jest-expo@53.0.13_@babel+core@7.28.5_expo@54.0.29_jest@29.7.0_@types+node@25.0.1__react_98642b21943c34ce6f99732034af3868/node_modules:/home/ahmad/Desktop/calmnest/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jest.js" "$@"
else
  exec node  "$basedir/../../bin/jest.js" "$@"
fi
